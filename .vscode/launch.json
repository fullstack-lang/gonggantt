    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Edge",
            "request": "launch",
            "type": "msedge",
            "url": "http://localhost:4200",
            "webRoot": "${workspaceFolder}/ng",
        },
        {
            "name": "go server unmarshallFromCode gantt1.go -marshallOnCommit gantt1",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/go/cmd/gonggantt",
            "args": [
                "-marshallOnCommit",
                "gantt1",
                "-unmarshallFromCode",
                "gantt1.go",
            ]
        },
        {
            "name": "go server unmarshallFromCode simple.go -marshallOnCommit simple",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/go/cmd/gonggantt",
            "args": [
                "-marshallOnCommit",
                "simple",
                "-unmarshallFromCode",
                "simple.go",
            ]
        },
    ]
}